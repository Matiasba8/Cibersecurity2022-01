    <div id="loginCard" style="border-color: gray; border-width: 3px">
        <h2 style="text-align: center; padding: 3%" id="saf_title">Inicio de sesión</h2>
        <div id="web2py_user_form">
            
               <!-- <form action="/user/login" enctype="multipart/form-data" method="post"> -->
            <%= form_with url: "user/login", method: "post" do |form| %>
                <table style="float: none; margin: 0px auto;">
                    <tbody>
                        <tr id="auth_user_email__row">
                            <td class="w2p_fl">
                                <label class="" for="auth_user_email" id="auth_user_email__label">Correo electrónico: </label>
                            </td>
                            <td class="w2p_fw">
                                <input class="string" id="auth_user_email" name="email" type="text">
                            </td>
                            <td class="w2p_fc">
                            </td>
                        </tr>
                        <tr id="auth_user_password__row">
                            <td class="w2p_fl">
                                <label class="" for="auth_user_password" id="auth_user_password__label">Contraseña: </label>
                            </td>
                            <td class="w2p_fw">
                                <input class="password" id="auth_user_password" name="password" type="password" value="">
                            </td>
                            <td class="w2p_fc">
                            </td>
                        </tr>
                        <tr id="auth_user_remember_me__row">
                            <td class="w2p_fl">
                                <label class="" for="auth_user_remember_me" id="auth_user_remember_me__label">No cerrar sesión: </label>
                            </td>
                            <td class="w2p_fw">
                                <input class="boolean" id="auth_user_remember_me" name="remember_me" type="checkbox" value="on">
                            </td>
                        </tr>
                        <tr id="submit_record__row">
                            <td class="w2p_fl">
                            </td>
                            <td class="w2p_fw">
                                <input type="submit" value="Ingresar" class="btn btn-primary center" id="submit_from_saf" style="vertical-align: central">
                            </td>
                            <td class="w2p_fc">
                            </td>
                        </tr>
                    </tbody>
                </table>
    
                <br>
                <br>
    
                <div class="container" id="bottom-form-container">
                    <div class="row">
                        <div class="span4 offset4" style="text-align: center;">
                            <a id="google_login_url">
                                <%= image_tag("btn_google_signin_dark_normal_web2x.png", class:"google-signin-responsive")%>
                            </a>
                            <br>
                            <small>*Necesitas una cuenta @miuandes.cl</small>
                            <br>
                            
                                <br>
                                <a href="">Restablecer contraseña</a>
                            
                        </div>
                    </div>
                </div>
                <br>
            <%end%>
        </div>
    </div>

            
